{
  "name": "ssr",
  "version": "0.14.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "deploy": "echo NOT_PUBLISHED"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.21",
    "@types/node-fetch": "^2.5.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^5.1.0",
    "@types/webpack-env": "^1.14.0",
    "@welldone-software/why-did-you-render": "^3.5.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "hoist-non-react-statics": "^3.3.0",
    "node-fetch": "^3.1.1",
    "node-stdev": "^1.0.1",
    "razzle": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-isomorphic-data": "^0.17.1",
    "react-router-dom": "^5.1.2",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0"
  },
  "devDependencies": {
    "@react-isomorphic-data/testing": "^0.13.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/babelJestTransformer.js",
      "^.+\\.jsx?$": "<rootDir>/babelJestTransformer.js",
      "\\.css$": "<rootDir>/../../node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/../../node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ]
  }
}
